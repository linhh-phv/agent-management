{
  "name": "agentManagement",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "setup-env-dev": "cp -rf .env.development ./ios/AgentManagement.xcconfig",
    "setup-env-staging": "cp -rf .env.staging ./ios/AgentManagement.xcconfig",
    "setup-env-prod": "cp -rf .env.production ./ios/AgentManagement.xcconfig",
    "android:dev": "react-native run-android --variant=developmentdebug --appIdSuffix=development",
    "android:dev-release": "react-native run-android --variant=developmentrelease --appIdSuffix=development",
    "android:staging": "react-native run-android --variant=stagingdebug --appIdSuffix=staging",
    "android:staging-release": "react-native run-android --variant=stagingrelease --appIdSuffix=staging",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "codepush:android:dev": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Dev",
    "codepush:android:staging": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Staging",
    "codepush:android:prod": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Production",
    "codepush:mandatory:android:dev": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Dev -m",
    "codepush:mandatory:android:staging": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Staging -m",
    "codepush:mandatory:android:prod": "appcenter codepush release-react -a linhh.phv/Agent-Management-Android-1 -d Production -m",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:dev": "yarn setup-env-dev && react-native run-ios --scheme \"agentManagement-Dev\"  --configuration DebugDev --simulator='iPhone 13'",
    "ios:dev-release": "yarn setup-env-dev && react-native run-ios --scheme \"agentManagement-Dev\"  --configuration ReleaseDev --simulator='iPhone 13'",
    "ios:staging": "yarn setup-env-staging && react-native run-ios --scheme \"agentManagement-Stage\"  --configuration DebugStage --simulator='iPhone 13'",
    "ios:staging-release": "yarn setup-env-staging && react-native run-ios --scheme \"agentManagement-Stage\"  --configuration ReleaseStage --simulator='iPhone 13'",
    "ios:prod": "yarn setup-env-prod && react-native run-ios --scheme \"agentManagement\"  --configuration Debug --simulator='iPhone 13'",
    "ios:prod-release": "yarn setup-env-prod && react-native run-ios --scheme \"agentManagement\"  --configuration Release --simulator='iPhone 13'",
    "ios:prod-release:device": "yarn setup-env-prod && react-native run-ios --scheme \"agentManagement\"  --configuration Release --udid='00008020-00180C3C0130003A'",
    "codepush:ios:dev": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Dev",
    "codepush:ios:staging": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Staging",
    "codepush:ios:prod": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Production",
    "codepush:mandatory:ios:dev": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Dev -m",
    "codepush:mandatory:ios:staging": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Staging -m",
    "codepush:mandatory:ios:prod": "appcenter codepush release-react -a linhh.phv/Agent-Management-iOS -d Production -m",
    "codepush:dev": "yarn codepush:android:dev && yarn codepush:ios:dev",
    "codepush:staging": "yarn codepush:android:staging && yarn codepush:ios:staging",
    "codepush:prod": "yarn codepush:android:prod && yarn codepush:ios:prod",
    "codepush:mandatory:dev": "yarn codepush:mandatory:android:dev && codepush:mandatory:ios:dev",
    "codepush:mandatory:staging": "yarn codepush:mandatory:android:staging && codepush:mandatory:ios:staging",
    "codepush:mandatory:prod": "yarn codepush:mandatory:android:prod && codepush:mandatory:ios:prod"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.5.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.13",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "metro-react-native-babel-preset": "^0.75.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
